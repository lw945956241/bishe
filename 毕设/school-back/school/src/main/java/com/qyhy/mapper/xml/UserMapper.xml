<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qyhy.mapper.UserMapper">

    <select id="queryBySessionId" resultType="com.qyhy.entity.User">
        select u.* from session s left join user u on s.user_id = u.id where s.user_id = #{sessionId}
    </select>

</mapper>
